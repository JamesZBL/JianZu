<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:state_pressed="true">
		<shape>
			<gradient android:angle="0" android:endColor="@color/color_taobao_dark_pr" android:startColor="@color/color_taobao_light_pr"/>
		</shape>
	</item>
	<item android:state_pressed="false">
		<shape>
			<gradient android:angle="0" android:endColor="@color/color_taobao_dark" android:startColor="@color/color_taobao_light"/>
		</shape>
	</item>
</selector>
